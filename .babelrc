{
  "plugins": [
    "transform-react-jsx",
    "transform-class-properties",
    "syntax-object-rest-spread",
    "transform-object-rest-spread"
  ],
  "env": {
    "web": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "> 1%",
                "last 2 Firefox versions",
                "last 2 Chrome versions",
                "last 2 Edge versions",
                "last 2 Safari versions",
                "Firefox ESR",
                "IE >= 8"
              ]
            }
          }
        ]
      ]
    },
    "node": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "7.6"
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}