{
  "name": "form-capacitor",
  "version": "0.1.27",
  "description": "Form manager for React/Redux",
  "main": "dist/index.js",
  "scripts": {
    "test": "node_modules/.bin/jest --coverage",
    "build": "rm -rf dist/* && babel src --out-dir dist",
    "prepublish": "yarn run build",
    "dev": "concurrently -k -n \"http,wp\" -c \"bgGreen.bold.white,bgBlue.bold.white\" -p \"{name}\" \"http-server -p 8456 ./examples/bootstrap\" \"webpack --config examples/bootstrap/webpack.config.js --watch\""
  },
  "keywords": [
    "form",
    "manager",
    "react",
    "redux"
  ],
  "author": "Mark Penner <mpenner@nucleuslabs.com>",
  "repository": "https://bitbucket.org/nucleuslabs/form-capacitor",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "recompose": "^0.30.0",
    "reselect": "^2.5.4",
    "shortid": "^2.2.6"
  },
  "peerDependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^5.1.1",
    "redux": "^3.7.2",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.28.11",
    "external-svg-sprite-loader": "^2.2.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "react": "^16.8.5",
    "prop-types": "^15.7.2",
    "react-dom": "^16.8.5",
    "react-redux": "^5.1.1",
    "redux": "^3.7.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "jest": "^24.5.0",
    "jest-extended": "^0.11.1",
    "react-testing-library": "^6.0.2",
    "webpack": "^2.3.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "collectCoverageFrom": [
      "**/src/*.js*"
    ]
  }
}
