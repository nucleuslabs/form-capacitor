{
  "name": "form-capacitor",
  "version": "0.1.18",
  "description": "Form manager for React/Redux",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist/* && babel src --out-dir dist",
    "prepublish": "npm run build",
    "dev": "concurrently -k -n \"http,wp\" -c \"bgGreen.bold.white,bgBlue.bold.white\" -p \"{name}\" \"http-server -p 8456 ./examples/bootstrap\" \"webpack --config examples/bootstrap/webpack.config.js --watch\""
  },
  "keywords": [
    "form",
    "manager",
    "react",
    "redux"
  ],
  "author": "Mark Penner <mpenner@nucleuslabs.com>",
  "repository": "https://bitbucket.org/nucleuslabs/form-capacitor",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "recompose": "^0.21.2",
    "reselect": "^2.5.4",
    "shortid": "^2.2.6"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-env": "^1.1.8",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "external-svg-sprite-loader": "^2.2.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "react-dom": "^15.4.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.3",
    "why-did-you-update": "0.0.8"
  }
}
