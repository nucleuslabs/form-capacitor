{
  "name": "form-capacitor",
  "version": "1.2.12",
  "description": "Strictly typed form state manager for React with as you type validation for many inputs with pretty errors using a json-schema definition and rules and a few simple Hooks",
  "browser": "web",
  "main": "dist/index.js",
  "module": "src/index.js",
  "keywords": [
    "form",
    "state",
    "manager",
    "errors",
    "validation",
    "react",
    "json-schema",
    "ajv",
    "mobx",
    "mobx-state-tree",
    "formik",
    "library"
  ],
  "contributors": [
    {
      "name": "Steve Ramage",
      "email": "sramage@nucleuslabs.com",
      "url": "https://nucleuslabs.com"
    },
    {
      "name": "Eric Jackisch",
      "email": "ejackisch@nucleuslabs.com",
      "url": "https://nucleuslabs.com"
    },
    {
      "name": "Mark Penner",
      "email": "mpenner@nucleuslabs.com",
      "url": "https://nucleuslabs.com"
    }
  ],
  "repository": "https://github.com/nucleuslabs/form-capacitor",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  },
  "dependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  },
