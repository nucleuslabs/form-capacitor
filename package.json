    "jest-extended": "^0.11",
    "json-schema-ref-parser": "^9",
    "lodash": "^4",
    "node-polyfill-webpack-plugin": "^1",
    "mobx": "^4",
    "mobx-react-lite": "^1",
    "mobx-state-tree": "^3",
    "webpack": "^5",
    "webpack-cli": "^4"
  },
  "scripts": {
    "test": "jest",
    "lint": "node_modules/.bin/eslint src/ --ext .js --format 'node_modules/eslint-codeframe-formatter'"
