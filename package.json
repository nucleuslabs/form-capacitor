{
  "name": "form-capacitor",
  "version": "0.2.0",
  "description": "Form manager for React/Redux",
  "main": "dist/index.js",
  "scripts": {
    "test": "node_modules/.bin/jest --coverage",
    "build": "rm -rf dist/* && node_modules/.bin/babel src --out-dir dist && cp package.json dist/package.json",
    "dev": "concurrently -k -n \"http,wp\" -c \"bgGreen.bold.white,bgBlue.bold.white\" -p \"{name}\" \"http-server -p 8456 ./examples/bootstrap\" \"webpack --mode development --config examples/bootstrap/webpack.config.js --watch\""
  },
  "keywords": [
    "form",
    "manager",
    "react",
    "redux"
  ],
  "author": "Mark Penner <mpenner@nucleuslabs.com>",
  "repository": "https://bitbucket.org/nucleuslabs/form-capacitor",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "recompose": "npm:react-recompose@0.33.0",
    "reselect": "^4.0.0",
    "shortid": "^2.2.6"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6",
    "@babel/core": "^7.6",
    "@babel/polyfill": "^7.6",
    "@babel/plugin-proposal-class-properties": "^7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6",
    "@babel/plugin-syntax-object-rest-spread": "^7.2",
    "@babel/plugin-transform-object-assign": "^7.2",
    "@babel/plugin-transform-react-jsx": "^7.3",
    "@babel/preset-env": "^7.6",
    "@babel/preset-react": "^7.6",
    "@testing-library/react": "^13",
    "babel-loader": "^8.0.0",
    "concurrently": "^3.1.0",
    "css-loader": "^3.2.0",
    "external-svg-sprite-loader": "^2.2.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "jest": "^26",
    "jest-extended": "^0.11.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^2.0.4",
    "react-redux": "^7.2.9",
    "redux": "^3.7.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "collectCoverageFrom": [
      "**/src/*.js*"
    ]
  }
}
